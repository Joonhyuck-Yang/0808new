# Railway 배포용 환경변수
PORT=8000
AUTH_SERVICE_URL=http://auth-service:8000

# Railway 환경 감지
RAILWAY_ENVIRONMENT=true

# Railway URL (배포 후 실제 URL로 변경 필요)
RAILWAY_URL=https://your-railway-domain.railway.app

# 로깅 레벨
LOG_LEVEL=INFO

# CORS 설정
ALLOWED_ORIGINS=https://jhyang.info,https://www.jhyang.info,http://localhost:3000

# Railway 로그 설정
RAILWAY_LOG_JSON=true
RAILWAY_LOG_LEVEL=INFO
