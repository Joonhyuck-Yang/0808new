# Auth Service Railway 배포용 환경변수
PORT=8000

# Railway 환경 감지
RAILWAY_ENVIRONMENT=true

# 로깅 레벨
LOG_LEVEL=INFO

# Gateway 서비스 URL (외부 API 호출 테스트용)
GATEWAY_URL=https://gateway-production-be21.up.railway.app

# Railway 로그 설정
RAILWAY_LOG_JSON=true
RAILWAY_LOG_LEVEL=INFO

# HTTP 클라이언트 설정
HTTP_TIMEOUT=30
HTTP_MAX_CONNECTIONS=100
HTTP_MAX_KEEPALIVE=20
